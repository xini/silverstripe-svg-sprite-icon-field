{
    "name": "innoweb/silverstripe-svg-sprite-icon-field",
    "description": "Adds an icon selector field to Silverstripe that uses an SVG sprite.",
    "type": "silverstripe-vendormodule",
    "keywords": ["silverstripe", "field", "svg", "svg-sprite", "icon",
        "client/icons"
    ],
    "license": "BSD-3-Clause",
    "authors": [{
        "name": "Florian Thoma",
        "homepage": "https://www.innoweb.com.au"
    }],
    "require":
    {
        "ext-dom": "*",
        "silverstripe/framework": "^5",
        "silverstripe/vendor-plugin": "^2"
    },
    "autoload": {
        "psr-4": {
            "Innoweb\\SvgSpriteIconField\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client"
        ]
    }
}
